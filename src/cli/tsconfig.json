{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": [
            "es6",
            "dom",
            "esnext.asynciterable"
        ],
        "allowJs": false,
        "declaration": true,
        "typeRoots": [
            "../../node_modules/@types"
        ],
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "outDir": "../../dist",
        "baseUrl": ".",
        "paths": {
            "quicktype-core": [
                "../quicktype-core"
            ]
        }
    }
}