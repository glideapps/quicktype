    "@types/semver": "^5.5.0",
    "@types/yaml": "^1.0.2",
    "ajv": "^5.5.2",
    "deep-equal": "^1.0.1",
    "elm": "0.19.1-5",
    "exit": "^0.1.2",
    "flow-bin": "^0.66.0",
    "flow-remove-types": "^1.2.3",
    "jest": "^23.1.0",
    "promise-timeout": "^1.3.0",
    "semver": "^5.5.0",
    "shelljs": "^0.7.8",
    "ts-jest": "^23.1.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "uglify-js": "^3.3.22",
    "watch": "^1.0.2",
    "@types/graphql": "^0.11.7",
    "@types/js-base64": "^2.3.1",
    "@types/pako": "^1.0.0",
    "@types/pluralize": "0.0.28",
    "@types/readable-stream": "2.3.9",
    "@types/browser-or-node": "^1.2.0"
  },
  "files": [
    "dist/**"
  ],
  "bin": "dist/cli/index.js",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/cli/tsconfig.json"
      }
    },
    "testRegex": "(/__tests__/.*)\\.test\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
