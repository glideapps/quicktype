    "pako": "^1.0.6",
    "pluralize": "^7.0.0",
    "stream-json": "1.1.3",
    "string-to-stream": "^1.1.0",
    "typescript": "~3.2.1",
    "@mark.probst/typescript-json-schema": "~0.32.0",
    "unicode-properties": "^1.3.1",
