    "@types/js-base64": "^2.3.1",
    "@types/pako": "^1.0.0",
    "@types/pluralize": "0.0.28"
  },
  "files": [
    "dist/**"
  ],
  "bin": "dist/cli/index.js",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/tsconfig.json"
      }
    },
    "testRegex": "(/__tests__/.*)\\.test\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
