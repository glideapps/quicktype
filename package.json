        "is-url": "^1.2.4",
        "js-base64": "^2.4.3",
        "lodash": "^4.17.10",
        "moment": "^2.22.1",
        "node-fetch": "^1.7.1",
        "node-persist": "^2.1.0",
        "pako": "^1.0.6",
        "pluralize": "^7.0.0",
        "stream-json": "1.1.3",
        "string-to-stream": "^1.1.0",
        "typescript-json-schema": "quicktype/typescript-json-schema#8ce897297cd101605a8f331e439e3281585ce12e",
        "unicode-properties": "github:quicktype/unicode-properties#dist",
        "universal-analytics": "^0.4.16",
        "urijs": "^1.19.1",
        "uuid": "^3.2.1",
        "wordwrap": "^1.0.0"
    },
    "devDependencies": {
        "@types/urijs": "github:quicktype/types-urijs",
        "@types/is-url": "^1.2.28",
        "@types/jest": "^23.1.6",
        "@types/lodash": "^4.14.108",
        "@types/node": "^8.10.10",
        "@types/node-persist": "0.0.33",
        "@types/shelljs": "^0.7.8",
        "@types/universal-analytics": "^0.4.2",
        "@types/semver": "^5.5.0",
        "ajv": "^5.5.2",
        "deep-equal": "^1.0.1",
        "elm": "^0.18.0",
        "exit": "^0.1.2",
        "flow-bin": "^0.66.0",
        "flow-remove-types": "^1.2.3",
        "jest": "^23.1.0",
        "pkg": "^4.3.1",
        "promise-timeout": "^1.3.0",
        "semver": "^5.5.0",
        "shelljs": "^0.7.8",
        "ts-jest": "^23.1.3",
        "ts-node": "^3.3.0",
        "tslint": "^5.9.1",
        "typescript": "~3.0.3",
        "uglify-js": "^3.3.22",
        "watch": "^1.0.2",
        "@types/graphql": "^0.11.7",
        "@types/js-base64": "^2.3.1",
        "@types/pako": "^1.0.0",
        "@types/pluralize": "0.0.28"
    },
    "files": [
        "dist/**"
    ],
    "bin": "dist/cli/index.js",
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsConfigFile": "src/cli/tsconfig.json"
            }
        },
        "testRegex": "(/__tests__/.*)\\.test\\.(tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
