    "compare-versions": "^3.1.0",
    "deep-equal": "^1.0.1",
    "elm": "^0.18.0",
    "exit": "^0.1.2",
    "flow-bin": "^0.66.0",
    "flow-remove-types": "^1.2.3",
    "jest": "^22.4.3",
    "pkg": "^4.3.1",
    "promise-timeout": "^1.3.0",
