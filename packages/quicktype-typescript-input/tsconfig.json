{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": ["es6", "esnext.asynciterable", "DOM"],
        "allowJs": false,
        "declaration": true,
        "strict": true,
        "alwaysStrict": true,
        "outDir": "dist",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "baseUrl": "src"
    },
    "include": ["src"]
}
