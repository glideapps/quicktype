  "version": "0.2.0",
  "configurations": [
    {
      "name": "quicktype",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/ts-node/dist/_bin.js",
      "args": [
        "--project",
        "src/tsconfig.json",
        "src/cli.ts",
        "--lang",
        "go",
        "test/inputs/json/samples/bitcoin-block.json"
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    },
    {
      "name": "test",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/ts-node/dist/_bin.js",
      "args": ["--project", "test/tsconfig.json", "test/test.ts"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "env": {
        "CPUs": "1",
        "FIXTURE": "golang"
      }
    }
  ]
}
